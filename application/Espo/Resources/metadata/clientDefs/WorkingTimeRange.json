{
    "controller": "controllers/record",
    "viewSetupHandlers": {
        "record/edit": "handlers/working-time-range"
    },
    "mergeDisabled": true,
    "massUpdateDisabled": true,
    "menu": {
        "list": {
            "buttons": [
                {
                    "name": "calendars",
                    "label": "Calendars",
                    "link": "#WorkingTimeCalendar"
                }
            ]
        }
    },
    "dynamicLogic": {
        "fields": {
            "timeRanges": {
                "visible": {
                    "conditionGroup": [
                        {
                            "type": "equals",
                            "attribute": "type",
                            "value": "Working"
                        }
                    ]
                }
            },
            "users": {
                "visible": {
                    "conditionGroup": [
                        {
                            "type": "or",
                            "value": [
                                {
                                    "type": "isNotEmpty",
                                    "attribute": "id"
                                },
                                {
                                    "type": "isNotEmpty",
                                    "attribute": "usersIds"
                                },
                                {
                                    "type": "isEmpty",
                                    "attribute": "calendarsIds"
                                }
                            ]
                        }
                    ]
                }
            }
        }
    },
    "filterList": [
        "actual"
    ]
}
